      jQuery("#hidden_[% nr %]").parent("TR").show();
    }
    [% END %]

    var max_entries_per_page = 32;
    render_total_sla_graph([% nr %], "[% loc("Availability") %]", graph_data, [% param.sla %], [% param.graph_min_sla %], "[% type | html %]", label, max_entries_per_page);
